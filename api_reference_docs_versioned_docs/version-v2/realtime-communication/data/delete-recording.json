{
  "deleteRecording": {
    "title": "Delete a Recording",
    "description": "Allows you to delete a recording via the recording ID.",
    "headers": [
      {
        "key": "Authorization",
        "value": "$YOUR_TOKEN"
      },
      {
        "key": "Content-Type",
        "value": "application/json"
      }
    ],
    "methodType": "DELETE",
    "postParameters": [],
    "queryParameters": [],
    "parameters": [
      {
        "key": "recordingId",
        "description": "#### The ID of the Recording.",
        "required": true
      }
    ],
    "apiEndpoint": "https://api.videosdk.live/v2/recordings/${recordingId}",
    "response": "Room recording deleted successfully"
  }
}